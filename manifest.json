{
  "manifest_version": 2,

  "name": "Frontendler inspector",
  "description": "Chrome plugin to inspect web page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/jquery.js", "js/content.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
